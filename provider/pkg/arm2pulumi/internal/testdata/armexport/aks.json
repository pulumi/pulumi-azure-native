{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "managedClusters_azure_nextgen_aks_name": {
      "defaultValue": "azure-nextgen-aks",
      "type": "String"
    },
    "publicIPAddresses_87a245ee_edc1_470c_a08e_c456659d9861_externalid": {
      "defaultValue": "/subscriptions/0282681f-7a9e-424b-80b2-96babd57a8a1/resourceGroups/MC_azure-nextgen-go_azure-nextgen-aks_westus/providers/Microsoft.Network/publicIPAddresses/87a245ee-edc1-470c-a08e-c456659d9861",
      "type": "String"
    }
  },
  "variables": {},
  "resources": [
    {
      "type": "Microsoft.ContainerService/managedClusters",
      "apiVersion": "2020-07-01",
      "name": "[parameters('managedClusters_azure_nextgen_aks_name')]",
      "location": "westus",
      "sku": {
        "name": "Basic",
        "tier": "Free"
      },
      "properties": {
        "kubernetesVersion": "1.16.10",
        "dnsPrefix": "azurenextgenprovider",
        "agentPoolProfiles": [
          {
            "name": "agentpool",
            "count": 3,
            "vmSize": "Standard_DS2_v2",
            "osDiskSizeGB": 30,
            "maxPods": 110,
            "type": "VirtualMachineScaleSets",
            "orchestratorVersion": "1.16.10",
            "nodeLabels": {},
            "mode": "System",
            "osType": "Linux"
          }
        ],
        "linuxProfile": {
          "adminUsername": "testuser",
          "ssh": {
            "publicKeys": [
              {
                "keyData": "ssh-rsa \n"
              }
            ]
          }
        },
        "servicePrincipalProfile": {
          "clientId": "265097a5-0e6d-4315-8c76-998e743f5e2d"
        },
        "addonProfiles": {
          "KubeDashboard": {
            "enabled": true
          }
        },
        "nodeResourceGroup": "[concat('MC_azure-nextgen-go_', parameters('managedClusters_azure_nextgen_aks_name'), '_westus')]",
        "enableRBAC": true,
        "networkProfile": {
          "networkPlugin": "kubenet",
          "loadBalancerSku": "Standard",
          "loadBalancerProfile": {
            "managedOutboundIPs": {
              "count": 1
            },
            "effectiveOutboundIPs": [
              {
                "id": "[parameters('publicIPAddresses_87a245ee_edc1_470c_a08e_c456659d9861_externalid')]"
              }
            ]
          },
          "podCidr": "10.244.0.0/16",
          "serviceCidr": "10.0.0.0/16",
          "dnsServiceIP": "10.0.0.10",
          "dockerBridgeCidr": "172.17.0.1/16",
          "outboundType": "loadBalancer"
        }
      }
    },
    {
      "type": "Microsoft.ContainerService/managedClusters/agentPools",
      "apiVersion": "2020-07-01",
      "name": "[concat(parameters('managedClusters_azure_nextgen_aks_name'), '/agentpool')]",
      "dependsOn": [
        "[resourceId('Microsoft.ContainerService/managedClusters', parameters('managedClusters_azure_nextgen_aks_name'))]"
      ],
      "properties": {
        "count": 3,
        "vmSize": "Standard_DS2_v2",
        "osDiskSizeGB": 30,
        "maxPods": 110,
        "type": "VirtualMachineScaleSets",
        "orchestratorVersion": "1.16.10",
        "nodeLabels": {},
        "mode": "System",
        "osType": "Linux"
      }
    }
  ]
}
